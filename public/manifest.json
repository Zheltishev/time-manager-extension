{
  "manifest_version": 3,
  "name": "Task manager",
  "description": "Control of time spent on tasks",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "favicon.png"
  },
  "permissions": [
      "tabs",
      "activeTab",
      "scripting",
      "storage",
      "alarms",
      "notifications"
  ]
}
